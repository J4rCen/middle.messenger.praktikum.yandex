{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">= 12.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "start": "tsc --noEmit && vite build && node server.mjs",
    "lint": "eslint src/**/*.ts && stylelint src/**/*.scss",
    "test": "mocha",
    "lint-test": "mocha && eslint src/**/*.ts && stylelint src/**/*.scss",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/chai": "^4.3.6",
    "@types/jsdom": "^21.1.3",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.4.8",
    "@types/sinon-chai": "^3.2.9",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "chai": "^4.3.8",
    "eslint": "^8.48.0",
    "esm-loader-css": "^1.0.5",
    "esm-loader-typescript": "^1.0.5",
    "esmock": "^2.5.1",
    "express": "^4.18.2",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "mocha": "^10.2.0",
    "node-esm-loader": "^0.2.5",
    "prettier": "^3.0.3",
    "sass": "^1.64.2",
    "sinon": "^16.0.0",
    "sinon-chai": "^3.7.0",
    "stylelint": "^15.10.3",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.5"
  },
  "dependencies": {
    "express-history-api-fallback": "^2.2.1",
    "handlebars": "^4.7.8",
    "nanoid": "^4.0.2",
    "path": "^0.12.7",
    "vite-plugin-handlebars": "^1.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-test"
    }
  }
}
